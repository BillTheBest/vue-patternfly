{
  "name": "vue-patternfly",
  "version": "0.0.0",
  "description": "Patternfly components for Vue.js 2",
  "jsnext": "dist/bundle.js",
  "main": "dist/bundle.common.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "purge": "rm -rf dist/",
    "compile": "rollup -c",
    "build": "npm run purge && npm run compile",
    "lint": "eslint src/* test/*",
    "beforePublish": "npm run lint && npm run build"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "vue",
    "patternfly"
  ],
  "author": "Massimiliano Torromeo <massimiliano.torromeo@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtorromeo/vue-patternfly.git"
  },
  "bugs": {
    "url": "https://github.com/mtorromeo/vue-patternfly/issues"
  },
  "homepage": "https://github.com/mtorromeo/vue-patternfly#readme",
  "dependencies": {
    "c3": "^0.4.11",
    "lodash": "^4.16.4",
    "patternfly": "^3.14.0",
    "vue": "^2.1.0",
    "vue-strap": "yuche/vue-strap#v2"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-import": "^1.16.0",
    "node-sass": "^3.10.1",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-vue": "^2.2.6"
  }
}
